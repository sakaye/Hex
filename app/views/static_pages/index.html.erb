<%= render 'hero' %>
<div class="row">
  <div class="span9">
    <div class="row">
      <div class="span9">
        <%= render 'carousel' %>
      </div>
    </div>
    <div class="row">
      <div id="new-cards" class="span5">
        <h3>New Cards</h3>
        <ul class="unstyled">
          <% @lastest_cards.each do |t| %>
            <li><%= link_to t.name, t %></li>
          <% end %>
        </ul>
      </div>
      <div id="live-streams" class="span4">
        <h3>Live Streams</h3>
      </div>
    </div>
  </div>
  <div class="span3">
    <div id="newest-decks" class="well well-small">
      <h4>Newest Decks</h4>
      <ul class="unstyled">
        <li>Coming Soon</li>
      </ul>
    </div>
  </div>
</div>
